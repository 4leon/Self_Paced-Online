def sleep_in(weekday, vacation):
  if vacation is True:
    return True
  elif weekday is False:
    return True
  else:
    return False

def diff21(n):
  if n > 21:
    return (n - 21) * 2
  else:
    return 21 - n

def near_hundred(n):
  if abs(n - 100) <= 10:
    return True
  elif abs(n - 200) <= 10:
    return True
  else: return False